{
  "name": "betro-js-client",
  "version": "0.0.1",
  "private": true,
  "description": "Client for betro api",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "test": "jest --coverage",
    "example:generate": "ts-node test/generateExample.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prijindal/betro-js-client.git"
  },
  "keywords": [
    "betro"
  ],
  "author": "Priyanshu Jindal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prijindal/betro-js-client/issues"
  },
  "homepage": "https://github.com/prijindal/betro-js-client#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "betro-js-lib": "git+https://prijindal@github.com/betro-app/betro-js-lib.git#70aaf165daa0f8b21d8c73ec76f4e631ddf75475",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}

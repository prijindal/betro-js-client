{
  "name": "@betro/client",
  "version": "0.0.3",
  "description": "Client for betro api",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "test": "jest --coverage",
    "example:generate": "ts-node test/generateExample.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prijindal/betro-js-client.git"
  },
  "keywords": [
    "betro"
  ],
  "author": "Priyanshu Jindal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prijindal/betro-js-client/issues"
  },
  "homepage": "https://github.com/prijindal/betro-js-client#readme",
  "dependencies": {
    "@betro/lib": "1.0.1",
    "axios": "^0.27.2",
    "casual": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-puppeteer": "^6.1.0",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "puppeteer": "^13.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}

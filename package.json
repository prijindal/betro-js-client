{
  "name": "betro-js-client",
  "version": "0.0.1",
  "private": true,
  "description": "Client for betro api",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier src test --write",
    "test": "jest --coverage",
    "example:generate": "ts-node test/generateExample.ts",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prijindal/betro-js-client.git"
  },
  "keywords": [
    "betro"
  ],
  "author": "Priyanshu Jindal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prijindal/betro-js-client/issues"
  },
  "homepage": "https://github.com/prijindal/betro-js-client#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "betro-js-lib": "git+https://git@github.com/betro-app/betro-js-lib.git#178a0cd57159904385d5c7802456c7580bf76022",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-puppeteer": "^6.0.0",
    "nodemon": "^2.0.14",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "puppeteer": "^10.4.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
